{
  "name": "imobiliaria",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo -p 4000",
    "build": "next build",
    "start": "next start -p 4000",
    "lint": "next lint",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "commit:patch": "npm run version:patch && git add . && git commit -m \"chore: bump version to $(node -p \"require('./package.json').version\")\"",
    "commit:minor": "npm run version:minor && git add . && git commit -m \"feat: bump version to $(node -p \"require('./package.json').version\")\"",
    "commit:major": "npm run version:major && git add . && git commit -m \"feat!: bump version to $(node -p \"require('./package.json').version\")\"",
    "deploy:patch": "npm run commit:patch && git push",
    "deploy:minor": "npm run commit:minor && git push",
    "deploy:major": "npm run commit:major && git push"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "embla-carousel-react": "^8.0.0",
    "framer-motion": "^10.16.16",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "next": "^15.3.2",
    "pg": "^8.11.3",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.5.2",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "tailwind-merge": "^2.2.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20",
    "@types/pg": "^8.10.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
